{
  "name": "derhansen/sf_banners",
  "type": "typo3-cms-extension",
  "description": "Banner-Management Extension based on Extbase and Fluid. Loads banners asynchronously using JavaScript.",
  "homepage": "https://github.com/derhansen/sf_banners",
  "support": {
    "issues": "https://github.com/derhansen/sf_banners/issues"
  },
  "authors": [
    {
      "name": "Torben Hansen",
      "email": "torben@derhansen.com",
      "homepage": "https://www.derhansen.com",
      "role": "Developer"
    }
  ],
  "keywords": ["TYPO3 CMS", "banners", "banner management", "fluid", "extbase"],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^12.3 || dev-main"
  },
  "require-dev": {
    "typo3/cms-backend": "^12.3 || dev-main",
    "typo3/cms-frontend": "^12.3 || dev-main",
    "typo3/cms-extbase": "^12.3 || dev-main",
    "typo3/cms-fluid": "^12.3 || dev-main",
    "sbuerk/typo3-cmscomposerinstallers-testingframework-bridge": "^0.1.0",
    "typo3/testing-framework": "~7.0@dev || dev-main",
    "friendsofphp/php-cs-fixer": "^3.12.0"
  },
  "replace": {
    "typo3-ter/sf-banners": "self.version"
  },
  "autoload": {
    "psr-4": {
      "DERHANSEN\\SfBanners\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DERHANSEN\\SfBanners\\Tests\\": "Tests"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true,
      "sbuerk/typo3-cmscomposerinstallers-testingframework-bridge": true
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "sf_banners",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Public"
    }
  }
}
